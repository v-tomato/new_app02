<header class="masthead2">
</header>  
<div class = "calendar-1">
  
<%= link_to 'スケジュールを入力', new_meeting_path %>
<br>
<%= month_calendar events: @meetings do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
    <div>
      <%= link_to meeting.title, meeting %>
    </div>
  <% end %>
<% end %>

</div>

