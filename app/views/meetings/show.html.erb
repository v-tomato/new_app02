<!--予定入力後の確認ページ-->
<header class="masthead">
<div class="container signup-container-2">
  <div class="row">
    <div class="col">　　

    <p>
      <strong>タイトル</strong>
      <%= @meeting.title %>
     </p>

     <p>
       <strong>開始時刻</strong>
       <%= @meeting.start_time.strftime("%Y-%m-%d %H:%M") %>
     </p>

     <p>
      <strong>内容</strong>
      <%= @meeting.content %>
     </p>

    <p>
      <%= link_to '変更', edit_user_meeting_path(@meetimg.id) %> |
      <%= link_to '削除', user_meeting_path(@meetimg.id), method: :delete %>|
      <%= link_to '戻る', user_meetings_path(@meetimg.id) %>
    </p>
　　
　　</div>
  </div>
</div>
</header>