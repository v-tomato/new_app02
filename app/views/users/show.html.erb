<header class="masthead2">
</header>  
<div class = "calendar-1">
  
 <div class = "schedule-enter">
  <%= link_to 'スケジュールを入力', new_meeting_path(id: current_user) %>
 </div>

<%= month_calendar events: @meetings do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
    <div>
      <%= link_to meeting.title, meeting_path(id: current_user) %>
    </div>
  <% end %>
<% end %>

</div>




